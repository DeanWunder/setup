#!/bin/bash

if [ -z "$1" ]; then
    echo "Usage: $0 <branch>"
fi

dir=`pwd`

for project in bai bidfood cts dlp linbeck prime rms swconnect swdeliveryv2 watertight; do
    cd $HOME/git/$project && git stash > /dev/null && gsw master > /dev/null
done

cd $dir && git stash > /dev/null && gsw "$1"
cb
