#!/bin/bash

if [ -z "$1" ]; then
    echo "Usage: $0 <branch>"
    exit 1
fi

project_name=`task_code_to_project_name $1` || { echo "Error: invalid branch name" ; exit 1; }

if [[ "$project_name" == "swdeliveryv2" ]]; then
    project_name="swdelivery"
fi
source "$HOME/.bashrc"
of firefox "https://$1.$project_name.qa.finao.com.au"
