#!/bin/bash

if [ -z "$1" ]; then
    echo "Usage: $0 <branches>"
    exit 1
fi

for branch in "$@"; do
    project_name=`task_code_to_project_name $branch` || { echo "Error: invalid branch name: $branch" ; exit 1; }

    if [[ "$project_name" == "swdeliveryv2" ]]; then
        project_name="swdelivery"
    fi
    source "$HOME/setup/of"
    of firefox "https://$branch.$project_name.qa.finao.com.au"
done
